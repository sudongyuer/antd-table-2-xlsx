  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/pkg-name.git"
  },
  "funding": "https://github.com/sponsors/antfu",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
